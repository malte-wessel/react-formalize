{
  "name": "react-formalize",
  "version": "0.2.0",
  "description": "Serialize react forms",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib dist",
    "build": "babel src --out-dir lib",
    "build:umd": "webpack src/index.js dist/react-formalize.js && NODE_ENV=production webpack src/index.js dist/react-formalize.min.js",
    "lint": "eslint src test examples",
    "test": "NODE_ENV=test karma start",
    "test:watch": "NODE_ENV=test karma start --no-single-run --auto-watch",
    "prepublish": "npm run lint && npm run test && npm run clean && npm run build && npm run build:umd"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/malte-wessel/react-formalize.git"
  },
  "keywords": [
    "react",
    "form",
    "serialze",
    "submit"
  ],
  "author": "Malte Wessel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/malte-wessel/react-formalize/issues"
  },
  "homepage": "https://github.com/malte-wessel/react-formalize",
  "devDependencies": {
    "babel": "^5.5.8",
    "babel-core": "^5.6.18",
    "babel-eslint": "^3.1.15",
    "babel-loader": "^5.1.4",
    "eslint": "^0.23",
    "eslint-config-airbnb": "0.0.6",
    "eslint-plugin-react": "^2.3.0",
    "expect": "^1.6.0",
    "karma": "0.12.*",
    "karma-chrome-launcher": "^0.2.0",
    "karma-cli": "^0.1.0",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.0.3",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.6.0",
    "mocha": "^2.2.5",
    "react": "^0.14.0-rc1",
    "react-dom": "^0.14.0-rc1",
    "rimraf": "^2.3.4",
    "webpack": "^1.9.6",
    "webpack-dev-server": "^1.8.2"
  },
  "dependencies": {
    "deepmerge": "^0.2.10",
    "invariant": "^2.0.0",
    "object-path": "^0.9.2"
  }
}
