{
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
    "indent": [2, 4, {"SwitchCase": 1}],
    "comma-dangle": 0,
    "react/jsx-boolean-value": 0,
    "react/no-multi-comp": 0,
    "react/prop-types": 0,
    "id-length": 0
  },
  "plugins": [
    "react"
  ]
}
